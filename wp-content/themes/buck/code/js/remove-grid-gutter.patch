+      $wp_content_dir = str_replace('\\', '/', WP_CONTENT_DIR);
+      $dir = str_replace($wp_content_dir, WP_CONTENT_URL, $dir, $count);
+    }
+
+
+    if($count < 1)
+    {
+      // if file is in ??? folder
+      $wp_dir = str_replace('\\', '/', ABSPATH);
+      $dir = str_replace($wp_dir, site_url('/'), $dir);
