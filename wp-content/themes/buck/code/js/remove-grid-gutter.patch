+                    $r = wp_parse_args( $args, $defaults );
+                    extract( $r, EXTR_SKIP );
+
+                    $output = '';
+
+                    $r['exclude'] = implode( ',', apply_filters('wp_list_pages_excludes', array()) );
+
+                    // Query pages.
+                    $r['hierarchical'] = 0;
+                    $args = array(
+                                'sort_column'       =>  'menu_order',
+                                'post_type'         =>  $post_type,
+                                'posts_per_page'    => -1,
+                                'post_status'       =>  'any',
