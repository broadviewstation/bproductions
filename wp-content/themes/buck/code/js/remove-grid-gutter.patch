+  */
+
+  function helpers_get_dir($file)
+  {
+    $dir = trailingslashit(dirname($file));
+    $count = 0;
+
+
+    // sanitize for Win32 installs
+    $dir = str_replace('\\', '/', $dir);
+
+
+    // if file is in plugins folder
